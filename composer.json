{
    "name": "tweakwise/magento2-tweakwise-js",
    "license": "OSL-3.0",
    "description": "Magento 2 module for Tweakwise JS",
    "type": "magento2-module",
    "require": {
        "php": "^8.1",
        "tweakwise/magento2-tweakwise-export": "^7.0"
    },
    "require-dev": {
        "magento/framework": "*",
        "emico/code-quality": "^10.5"
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Tweakwise\\TweakwiseJs\\": "src/"
        }
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        "emico/code-quality": {
            "type": "vcs",
            "url": "git@git.emico.io:composer-packages/generic/codequality.git"
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": false,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "emico/code-quality": true,
            "phpro/grumphp": true
        }
    }
}
